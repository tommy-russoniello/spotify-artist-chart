{
  "name": "Spotify Artist Chart",
  "version": "0.2.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://*.spotify.com/artist/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "webRequest",
    "storage",
    "declarativeContent"
  ],
  "host_permissions": [
    "*://*.spotify.com/"
  ],
  "action": {
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
